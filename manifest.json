{
  "name": "VK old-unread",
  "version": "1.3.0",
  "description": "Небольшое расширение, призванное сделать новый дизайн ВК чуточку лучше.",
  "manifest_version": 3,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://vk.com/*"
      ],
      "css": [
        "mystyles.css"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/vkou.js",
        "/vkou_lib.js",
        "/oldbg.css",
        "/newbg.css"
      ],
      "matches": [
        "https://vk.com/*"
      ]
    }
  ]
}
