{
    "name": "VK old-unread",
    "version": "1.1.0.7",
	"description": "Небольшое расширение, призванное сделать новый дизайн ВК чуточку лучше.",
 
    "manifest_version": 2,
    "icons": {
        "128": "icon.png"
    },
	"content_scripts": [
		{
			"matches": [ "https://vk.com/*" ],
			"css": ["mystyles.css"],
			"js": [ "background.js" ],
			"run_at": "document_end"
		}
	],
	
	"web_accessible_resources": [
		"/vkou.js", "/oldbg.css", "/newbg.css"
	]
}
